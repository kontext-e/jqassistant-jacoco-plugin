<jqa-plugin:jqassistant-plugin xmlns:jqa-plugin="http://www.buschmais.com/jqassistant/core/plugin/schema/v1.0" name="JaCoCo">
    <description>Provides a scanner for JaCoCo XML test coverage result files.</description>
    <model>
        <class>de.kontext_e.jqassistant.plugin.jacoco.store.descriptor.JacocoDescriptor</class>
        <class>de.kontext_e.jqassistant.plugin.jacoco.store.descriptor.JacocoReportDescriptor</class>
        <class>de.kontext_e.jqassistant.plugin.jacoco.store.descriptor.JacocoPackageDescriptor</class>
        <class>de.kontext_e.jqassistant.plugin.jacoco.store.descriptor.JacocoClassDescriptor</class>
        <class>de.kontext_e.jqassistant.plugin.jacoco.store.descriptor.JacocoMethodDescriptor</class>
        <class>de.kontext_e.jqassistant.plugin.jacoco.store.descriptor.JacocoCounterDescriptor</class>
    </model>
    <scanner>
        <class>de.kontext_e.jqassistant.plugin.jacoco.scanner.JacocoScannerPlugin</class>
    </scanner>
  <rules>
      <resource>jacoco.xml</resource>
  </rules>

</jqa-plugin:jqassistant-plugin>
